{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/api/index.js"
    },
    {
      "src": "/scenarios(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/simulate(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/report(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
